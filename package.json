	"module": "dist/cs.esm.js",
	"size-limit": [
		{
			"path": "dist/cs.cjs.production.min.js",
			"limit": "1 KB"
		},
		{
			"path": "dist/cs.esm.js",
			"limit": "1 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.9.2",
		"@types/lodash.isplainobject": "^4.0.6",
		"husky": "^5.0.9",
		"size-limit": "^4.9.2",
		"tsdx": "^0.14.1",
		"tslib": "^2.1.0",
		"typescript": "^4.1.5"
	},
	"dependencies": {
		"lodash.isplainobject": "^4.0.6"
	}
